    "a138a290-ba33-4f68-8c4d-ba44e3f0af30": {
      "type": "timeline-custom",
      "blocks": {
        "394c8aa5-d50e-409c-83c3-fcff66b4fedd": {
          "type": "timeline",
          "settings": {
            "timeline_text": "Upon a successful order and payment confirmation, you will receive your order details via email, typically within 1-3 minutes. Please add support@freemypaws to your email provider's safe sender list or check your spam folder after 5 minutes post order if you have not received this.",
            "button_title": "Step 1:",
            "button_title_size": "H4"
          }
        },
        "f93b6f21-4035-41ec-83b9-11d9c87207bb": {
          "type": "timeline",
          "settings": {
            "timeline_text": "Our system prepares your profile and, via email, sends out to the email we have on file for you a link to the Client Questionnaire for Therapist Confirmation for your completion. The online Client Questionnaire is a secure web-based form to complete and submit to us electronically. Again, this is sent out via our system, typically within 15 minutes of a successful order. Please add support@freemypaws to your email provider's safe sender list or check your spam folder 20 minutes post-order if you have not received this.",
            "button_title": "Step 2:",
            "button_title_size": "H4"
          }
        },
        "5b73c627-f5fb-4aaa-9f91-ad074a3f9a10": {
          "type": "timeline",
          "settings": {
            "timeline_text": "After you have completed and submitted back electronically the online Client Questionnaire, the assigned Therapist will make contact with you to schedule a phone conversation. The typical time frame for this conversation is between 10-15 minutes. You should expect to hear from the Therapist assigned to your account within 48 hours but typically, within 24 hours is the norm. Orders placed over the weekend or on a national holiday will be attended to the following business day.",
            "button_title": "Step 3:",
            "button_title_size": "H4"
          }
        },
        "e19fbba7-1924-441d-a97d-43e6e4ba74ec": {
          "type": "timeline",
          "settings": {
            "timeline_text": "Immediately after your telephonic conversation with the Therapist, your Client Questionnaire will either be confirmed or denied. If confirmed, your Therapist-Certified Letter and supporting documents will be emailed to you for your usage. If denied, you will be issued a refund for your purchase amount within 3-4 business days by our accounting department.If you have any questions about the process outlined above, don't hesitate to contact us using our online inquiry form or at support@freemypaws.com.",
            "button_title": "Step 4:",
            "button_title_size": "H4"
          }
        }
      },
