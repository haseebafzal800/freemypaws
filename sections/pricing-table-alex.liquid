        {% elsif section.settings.pricing_title_size == "H6" %}
          <h6>{{ section.settings.pricing_title }}</h6>
        {% endif %}
        {{ section.settings.pricing_txt }}
        <h2>{{ section.settings.price }}</h2>
        <ul class="pricing_fetures">
          {%- if section.settings.feature_1 != blank -%}
