<style>
  .section-{{ section.id }} .pricing-title {
    color: {{ section.settings.title_color }};
  }
  .section-{{ section.id }} .pricing-title span {
    color: {{ section.settings.highlighted_color }};
  }
  .section-{{ section.id }} .pricing-content p {
    color: {{ section.settings.paragraph_color }};
  }
  .section-{{ section.id }} .tbl_wrap {
    background-color: {{ section.settings.tbl_bg }};
    color: {{ section.settings.tbl_txt_color }};
  }
  .section-{{ section.id }} .tbl_wrap svg path {
    fill: {{ section.settings.tbl_txt_color }};
  }
  span.highlight-title {
    color: #388E3C!important;
  }
  .pricing_fetures {
    text-align: left;
    padding-right: 30px;
    padding-left: 30px;
  }
  .section-{{ section.id }} div.pricing_btn a:hover {
    transform: translate(-50%, 50%);
  }
</style>
<div class="sec-pricing-table section-{{ section.id }}">
  <div class="container d-flex">
    <div class="pricing-content">
      <h2 class="pricing-title">
        {{ section.settings.title }}
        <span class="highlight-title">
          <em>{{ section.settings.highlight_title }}</em>
        </span>
      </h2>
      <p>{{ section.settings.paragraph }}</p>
    </div>
    <div class="pricing-table d-flex">
      {% if section.settings.enable_tbl1 %}
